    "scripts": {
        "start": "env DEBUG=provider:* node src/lti.js",
        "build": "parcel build --no-scope-hoist --no-optimize --no-content-hash src/index.html",
        "start-parcel": "NO_LTI=1 TE_CANVAS_URL=http://localhost:5000 parcel --port $PARCEL_PORT src/index.html"
    },
    "browserslist": "defaults",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@instructure/ui": "^8.16.0",
        "ltijs": "^5.8.6",
        "parcel": "^2.0.0-rc.0",
        "prettier": "^2.3.2",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.2.1"
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "importOrder": ["^react", "^@instructure", "^[^.]", ".*"],
        "importOrderSeparation": true,
        "importOrderSortSpecifiers": true
